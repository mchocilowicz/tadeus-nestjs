{
  "name": "tadeus",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "12.4.0",
    "npm": "6.9.0"
  },
  "scripts": {
    "build": "npm run typeorm-migration-run",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "prestart:prod": "rimraf prod && npm run build",
    "start:prod": "pm2 start dist/main.js",
    "typeorm-migration-generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "typeorm-migration-run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "typeorm-migration-revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "typeorm-migration-create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.1.2",
    "@types/compression": "^1.0.1",
    "@types/express": "^4.16.0",
    "@types/express-rate-limit": "^3.3.3",
    "@types/helmet": "0.0.44",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.136",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.10",
    "@types/node": "^10.12.18",
    "@types/passport-jwt": "^3.0.2",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "compression": "^1.7.4",
    "csurf": "^1.10.0",
    "express-locale": "^1.2.0",
    "express-rate-limit": "^5.0.0",
    "helmet": "^3.18.0",
    "i18next": "^17.0.6",
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nest-morgan": "^1.0.1",
    "nest-router": "^1.0.9",
    "nest-schedule": "^0.6.3",
    "node-polyglot": "^2.3.1",
    "nodemailer": "^6.3.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.11.0",
    "pm2": "^4.1.2",
    "pretty-error": "^2.1.1",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "swagger-ui-express": "^4.0.6",
    "ts-jest": "^24.0.2",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "twilio": "^3.32.0",
    "typeorm": "^0.2.18",
    "typescript": "3.4.3",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "xlsx": "^0.14.4"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.5.3",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "tslint-config-prettier": "^1.18.0",
    "wait-on": "^3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
